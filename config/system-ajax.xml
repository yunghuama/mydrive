<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
  
  <package name="systemAjax" extends="json-default" namespace="/system/ajax">
  
    <action name="saveScore" class="questionAjaxAction" method="saveScore">
      <result type="json">
        <param name="root">result</param>
      </result>
    </action>
	<action name="saveMarkQuestion" class="questionAjaxAction" method="saveMarkQuestion">
      <result type="json">
        <param name="root">result</param>
      </result>
    </action>
    <action name="saveScore3" class="questionAjaxAction" method="saveScore3">
      <result type="json">
        <param name="root">result</param>
      </result>
    </action>
	<action name="saveMarkQuestion3" class="questionAjaxAction" method="saveMarkQuestion3">
      <result type="json">
        <param name="root">result</param>
      </result>
    </action>
    <action name="statisticsSub1" class="questionAjaxAction" method="statisticsSub1">
      <result type="json">
        <param name="root">statisticVO</param>
      </result>
    </action>
    <action name="statisticsSub3" class="questionAjaxAction" method="statisticsSub3">
      <result type="json">
        <param name="root">statisticVO</param>
      </result>
    </action>
    <action name="scoreListSub1" class="questionAjaxAction" method="scoreListSub1">
      <result type="json">
        <param name="root">scoreVoList</param>
      </result>
    </action>
    <action name="scoreListSub3" class="questionAjaxAction" method="scoreListSub3">
      <result type="json">
        <param name="root">scoreVoList</param>
      </result>
    </action>
  </package>
  
</struts>