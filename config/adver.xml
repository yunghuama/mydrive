<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
  <package name="adver" extends="role-default" namespace="/system/adver">
  	<action name="list" class="adverAction" method="list">
      <result>/adver/ad_list.jsp</result>
    </action>
    <action name="save" class="adverAction" method="save">
      <result type="redirect">/system/adver/list.d</result>
    </action>
    <action name="delete" class="adverAction" method="delete">
      <result type="redirect">/system/adver/list.d</result>
    </action>
  </package>
</struts>